{
  "name": "video-lightning",
  "title": "Video Lightning",
  "version": "4.0.0-nonfunctional",
  "description": "Turn any element into a lightbox or popover link for Youtube and Vimeo videos.",
  "files": [
    "index.js",
    "module.js"
  ],
  "main": "index.js",
  "module": "module.js",
  "scripts": {
    "test": "rollup -c && ava --verbose",
    "start": "rollup -c && ava --verbose",
    "precommit": "rollup -c && ava && git add index.js module.js"
  },
  "keywords": [
    "jquery",
    "jquery-plugin",
    "ecosystem:jquery",
    "video",
    "lightning",
    "lightbox",
    "popover",
    "popup",
    "youtube",
    "vimeo",
    "api",
    "rick",
    "roll",
    "rickroll",
    "hover",
    "preview",
    "iframe"
  ],
  "author": "Andrew Carpenter (https://github.com/doesdev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/doesdev/video-lightning.git"
  },
  "bugs": {
    "url": "https://github.com/doesdev/video-lightning/issues"
  },
  "homepage": "https://github.com/doesdev/video-lightning/",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.2.4",
    "husky": "^0.14.3",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.3"
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
